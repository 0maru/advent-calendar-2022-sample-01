root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
end_of_line = lf
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.dart]
max_line_length = 100

[*.swift]
indent_size = 4

[*.m]
indent_size = 4

[*.kt]
indent_size = 4

[*.java]
indent_size = 4